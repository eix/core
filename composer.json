{
  "name": "eix/core",
  "description": "Eix is a PHP-based framework for building web applications.",
  "type": "library",
  "keywords": [
    "framework"
  ],
  "homepage": "http://eix.0h.io/",
  "time": "2013-01-01",
  "license": "MIT",
  "authors": [
    {
      "name": "Max NoÃ©",
      "email": "github@0h.io",
      "role": "Developer"
    }
  ],
  "support": {
    "issues": "https://github.com/eix/core",
    "source": "https://github.com/eix/core"
  },
  "prefer-stable": true,
  "config": {
    "vendor-dir": "src/php/lib"
  },
  "autoload": {
    "psr-0": {
      "Eix\\": "src/php/main"
    }
  },
  "autoload-dev": {
    "psr-0": {
      "Eix\\": "src/php/test"
    }
  },
  "require": {
    "php": ">=5.3",
    "ext-xsl": ">0",
    "ext-intl": ">0",
    "ext-json": ">0"
  },
  "require-dev": {
    "phing/phing": "*",
    "phpunit/phpunit": "~4.7",
    "phplint/phplint": "*",
    "phploc/phploc": "*",
    "pdepend/pdepend": "*",
    "phpmd/phpmd": "*",
    "sebastian/phpcpd": "*",
    "mayflower/php-codebrowser": "*"
  }
}
