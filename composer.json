{
    "name": "eix/core",
    "description": "Eix is a PHP-based framework for building web applications.",
    "type": "library",
    "keywords": ["framework"],
    "homepage": "http://eix.nohex.com/",
    "time": "2013-01-01",
    "license": "MIT",
    "authors": [
        {
            "name": "Max NoÃ©",
            "email": "github@0h.com",
            "role": "Developer"
        }
    ],
	"require": {
		"php": ">=5.3",
		"ext-xsl": ">0",
		"ext-intl": ">0",
		"ext-json": ">0"
	},
    "require-dev": {
        "phpunit/phpunit": "~4.7"
    },
    "support": {
        "issues": "https://github.com/eix/core",
        "source": "https://github.com/eix/core"
    },
    "autoload": {
        "psr-0": {
            "Eix\\": [
				"src/php/main",
				"src/php/test"
			]
        }
    },
    "prefer-stable": true,
	"config": {
		"vendor-dir": "src/php/lib"
	}
}
